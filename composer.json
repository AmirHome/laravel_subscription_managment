{
    "name": "amirhome/laravel_subscription_managment",
    "description": "laravel_subscription_managment is a flexible plan and subscription management system for Laravel, offering essential tools to efficiently run your SAAS-like services. With a simple architecture and robust foundation, it provides a solid platform to effortlessly manage plans and subscriptions.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Amirhome\\LaravelSubscriptionManagment\\": "src/"
        },
        "files": [
            "src/Helper/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "amirhome",
            "email": "amir.email@yahoo.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0 || ^11.0",
        "yajra/laravel-datatables": "^10.1 || ^11.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Amirhome\\LaravelSubscriptionManagment\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amirhome\\LaravelSubscriptionManagment\\LaravelSubscriptionManagmentServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/amirhome/laravel_subscription_managment/issues",
        "source": "https://github.com/amirhome/laravel_subscription_managment"
    },
    "homepage": "https://github.com/amirhome/laravel_subscription_managment",
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.20",
        "orchestra/testbench": "^9.9"
    }
}
